<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.camping.dev.mapper.SampleMapper">

    <select id="getSample" resultType="com.camping.dev.model.vo.SampleVO">
        <![CDATA[
            select aaa, bbb, ccc, ddd
            from sample
            where aaa=#{aaa}
        ]]>
    </select>


</mapper>